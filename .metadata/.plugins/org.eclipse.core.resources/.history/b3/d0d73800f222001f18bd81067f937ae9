        // Sudoku tahtasını tanımla
        char[][] board = {
            {'5', '3', '.', '.', '7', '.', '.', '.', '.'},
            {'6', '.', '.', '1', '9', '5', '.', '.', '.'},
            {'.', '9', '8', '.', '.', '.', '.', '6', '.'},
            {'8', '.', '.', '.', '6', '.', '.', '.', '3'},
            {'4', '.', '.', '8', '.', '3', '.', '.', '1'},
            {'7', '.', '.', '.', '2', '.', '.', '.', '6'},
            {'.', '6', '.', '.', '.', '.', '2', '8', '.'},
            {'.', '.', '.', '4', '1', '9', '.', '.', '5'},
            {'.', '.', '.', '.', '8', '.', '.', '7', '9'}
        };

       
   

        // Çözülmüş Sudoku tahtasını yazdır
       
    }

    public char[][] solveSudoku(char[][] board) {
    	
    	
    	for(int i = 0; i<board.length; i++) {
    		for(int j = 0; j<board.length; j++) {
    			
    			if(board[i][j] == '.') {
    				
    				board[i][j] = change(board[i][j], i, j, board);
    			}
    		}
    	}
    	
    	
    	
    	return board;
     
    }
    
    
    public char change() {
    	
    }

   
}
